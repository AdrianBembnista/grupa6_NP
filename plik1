#include <stdio.h>


void main()
{
float x,liczby1,liczby2;
printf("Podaj cztery liczby: ");
int i;
for(i=0;i<4;i++)
{
scanf ("%f",&x);
if(x>0)
{
 liczby1++;
}
else if(x<0)
{
liczby2++;
}

}

if (liczby1>liczby2)
{
    printf("dodatnich jest wiecej");
}
else
{

    printf("ujemnych jest wiecej");

}
fflush(stdin);
getchar();



}
